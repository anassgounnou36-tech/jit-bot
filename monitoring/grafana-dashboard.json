{
  "dashboard": {
    "id": null,
    "title": "JIT Bot Production Monitoring",
    "tags": ["jit-bot", "production", "mev"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "JIT Opportunities",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(jit_opportunities_detected_total[5m])",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "value"
            },
            "mappings": [],
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 80
                }
              ]
            },
            "unit": "ops"
          }
        },
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "horizontal",
          "reduceOptions": {
            "values": false,
            "calcs": [
              "lastNotNull"
            ],
            "fields": ""
          },
          "textMode": "auto"
        },
        "pluginVersion": "8.0.0",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Successful JIT Executions",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(jit_executions_success_total[5m])",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "value"
            },
            "mappings": [],
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 80
                }
              ]
            },
            "unit": "ops"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Profit Over Time",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(jit_profit_usd_total[5m])",
            "refId": "A",
            "legendFormat": "Profit USD/sec"
          }
        ],
        "yAxes": [
          {
            "label": "USD",
            "show": true
          },
          {
            "show": true
