{
  "name": "jit-bot",
  "version": "1.0.0",
  "description": "Just-In-Time Liquidity Provision bot for Uniswap V3",
  "main": "index.js",
  "scripts": {
    "build": "hardhat compile",
    "test": "hardhat test",
    "test:coverage": "hardhat coverage",
    "test:unit": "mocha",
    "test:fork": "ts-mocha test/integration/**/*.test.ts --timeout 30000",
    "test:all": "npm run test:unit && npm run test:fork",
    "test:solidity": "hardhat test",
    "start": "node dist/bot/index.js",
    "dev": "ts-node src/bot/index.ts",
    "live": "NODE_ENV=production ts-node src/bot/index.ts start",
    "deploy": "hardhat run scripts/deploy.ts",
    "deploy:fork": "hardhat run scripts/deploy.ts --network fork",
    "deploy:mainnet": "hardhat run scripts/deploy.ts --network mainnet",
    "simulate": "ts-node scripts/simulateExample.ts",
    "fork": "ts-node scripts/startFork.ts",
    "fork:simulate": "ts-node scripts/forkSimulation.ts",
    "fixtures:generate": "node scripts/generate-fixtures.js",
    "fixtures:validate": "node scripts/validate-fixtures.js",
    "fixtures:update": "npm run fixtures:generate",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "lint:solidity": "npm run build",
    "slither": "slither contracts/ --json slither-report.json || echo 'Slither analysis completed'",
    "ci": "npm run lint && npm run test:unit",
    "ci:e2e": "npm run test:fork && npm run test:unit && npm run slither"
  },
  "keywords": [
    "ethereum",
    "uniswap",
    "jit",
    "mev",
    "flashloan",
    "defi"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@typechain/ethers-v5": "^10.2.1",
    "@typechain/hardhat": "^6.1.6",
    "@types/chai": "^4.3.20",
    "@types/chai-as-promised": "^8.0.2",
    "@types/express": "^5.0.3",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.0.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "chai": "^4.5.0",
    "chai-as-promised": "^8.0.2",
    "eslint": "^8.0.0",
    "hardhat": "^2.19.0",
    "hardhat-gas-reporter": "^1.0.9",
    "mocha": "^11.7.2",
    "pino-pretty": "^13.1.1",
    "solidity-coverage": "^0.8.5",
    "ts-mocha": "^11.1.0",
    "ts-node": "^10.9.0",
    "typechain": "^8.3.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.9.0",
    "@types/uuid": "^10.0.0",
    "@uniswap/v3-core": "^1.0.1",
    "@uniswap/v3-periphery": "^1.4.3",
    "@uniswap/v3-sdk": "^3.10.0",
    "dotenv": "^16.3.0",
    "ethers": "^5.7.2",
    "express": "^4.18.0",
    "pino": "^9.9.5",
    "prom-client": "^15.1.3",
    "uuid": "^13.0.0",
    "ws": "^8.14.0"
  }
}
