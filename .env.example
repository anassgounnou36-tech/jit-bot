# ==================================================
# JIT Bot Configuration
# ==================================================

# Runtime Environment
NODE_ENV=development

# Blockchain Configuration
CHAIN=ethereum

# RPC Endpoints
# HTTP endpoint for read operations and gas estimation
RPC_URL_HTTP=https://eth-mainnet.alchemyapi.io/v2/YOUR_ALCHEMY_KEY
# WebSocket endpoint for real-time mempool monitoring
RPC_URL_WS=wss://eth-mainnet.ws.alchemyapi.io/v2/YOUR_ALCHEMY_KEY

# External API Keys
ETHERSCAN_API_KEY=
BLOCKNATIVE_API_KEY=

# Swap Detection Thresholds
# Minimum swap size in ETH to consider
MIN_SWAP_ETH=10
# Minimum swap size in USD to consider (0 = ETH threshold only)
MIN_SWAP_USD=0

# Profit Configuration
# Global minimum profit in USD required to execute opportunities
GLOBAL_MIN_PROFIT_USD=20
# Maximum gas price in Gwei for transaction execution
MAX_GAS_GWEI=100
# Percentage of fees to capture (0.65 = 65%)
CAPTURE_RATIO=0.65
# Risk buffer in USD to subtract from profit estimates
RISK_BUFFER_USD=0

# DRY RUN SAFETY (MANDATORY)
# DRY_RUN=true prevents actual bundle submission - ALWAYS START WITH THIS
DRY_RUN=true
# Must be set to true along with DRY_RUN=false for live execution
I_UNDERSTAND_LIVE_RISK=false
# Minimum ETH balance required before allowing live submissions
MIN_REQUIRED_ETH=0.005

# Flashloan Configuration
# Balancer preferred when available and cheaper, Aave as fallback
FLASHLOAN_PRIORITY=balancer-first
# Allow reconstruction of raw tx if eth_getRawTransactionByHash fails
ALLOW_RECONSTRUCT_RAW_TX=false

# Wallet Configuration
# Main wallet private key for transaction signing  
PRIVATE_KEY=0x...
# Flashbots signing key (MUST be different from PRIVATE_KEY)
FLASHBOTS_SIGNING_KEY=

# Flashbots Configuration
# Flashbots relay URL for MEV bundle submission
FLASHBOTS_RELAY_URL=https://relay.flashbots.net

# Monitoring & Metrics
# Prometheus metrics server port
PROMETHEUS_PORT=9090

# Pool Configuration
# Comma-separated list of pool identifiers to monitor
POOL_IDS=WETH-USDC-0.05%,ETH-USDT-0.3%,WBTC-ETH-0.3%

# ==================================================
# DEPRECATED VARIABLES (will show warnings)
# ==================================================
# SIMULATION_MODE - use DRY_RUN instead
# ENABLE_LIVE_EXECUTION - use DRY_RUN=false instead  
# ENABLE_FORK_SIM_PREFLIGHT - always enabled now
# METRICS_PORT - use PROMETHEUS_PORT instead

# ==================================================
# Additional Configuration (Optional)
# ==================================================

# Multi-Pool Settings
POOL_MAX_FAILURES=5
POOL_COOLDOWN_MS=300000
MAX_CONCURRENT_WATCHERS=10

# Per-Pool Profit Thresholds (optional overrides)
POOL_PROFIT_THRESHOLD_USD__WETH_USDC_0_05_=150
POOL_PROFIT_THRESHOLD_USD__ETH_USDT_0_3_=120
POOL_PROFIT_THRESHOLD_USD__WBTC_ETH_0_3_=200

# Bot Behavior
MIN_PROFIT_THRESHOLD=0.01
MAX_LOAN_SIZE=1000000
TICK_RANGE_WIDTH=60
GAS_PRICE_MULTIPLIER=1.1

# Simulation & Testing
TARGET_POOLS=WETH-USDC-0.05%,ETH-USDT-0.3%,WBTC-ETH-0.3%
SIMULATION_GAS_PRICE_GWEI=20
SIMULATION_REPORT_DIR=./reports

# External APIs
TENDERLY_API_KEY=

# Deployment Configuration
VERIFY_CONTRACTS=false
DEPLOYMENT_NETWORK=fork
JIT_CONTRACT_ADDRESS=